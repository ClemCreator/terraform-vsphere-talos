                  annotations:
                    prometheus.io/path: "/metrics"
                    prometheus.io/scrape: 'true'
                    prometheus.io/port: '9402'
                spec:
                  serviceAccountName: cert-manager-cainjector
                  enableServiceLinks: false
                  securityContext:
                    runAsNonRoot: true
                    seccompProfile:
                      type: RuntimeDefault
                  containers:
